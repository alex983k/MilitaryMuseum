<Window x:Class="Interface.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300">
    <Grid>
        <TextBox Name="UsernameTextBox"  HorizontalAlignment="Left" Height="23" Margin="39,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged_1"/>
        <TextBox Name="PasswordTextBox" HorizontalAlignment="Left" Height="23" Margin="39,125,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Label Content="Insert username:" HorizontalAlignment="Left" Margin="39,49,0,0" VerticalAlignment="Top" Width="109"/>
        <Label Content="Insert password:" HorizontalAlignment="Left" Margin="39,99,0,0" VerticalAlignment="Top"/>
        <Button Name="LogInButton" Content="Log in" HorizontalAlignment="Left" Margin="39,166,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-1.587,0.1" Click="Button_Click"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="155,166,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>

    </Grid>
</Window>
